    "from tkinter import *\n",
    "import time\n",
    "\n",
    "\n",
    "def parpadear():\n",
    "    ventana.iconify()\n",
    "    time.sleep(2)\n",
    "    ventana.deiconify()\n",
    "\n",
    "    \n",
    "ventana = Tk()\n",
    "boton = Button(ventana,text=\"Evento\",command = parpadear)\n",
    "boton.pack()\n",
    "ventana.mainloop()\n"
   ]
